{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-rearn-2s"
		},
		"ls_adls_sps_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_sps'"
		},
		"ls_sql_db_retaildb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_db_retaildb'"
		},
		"AzureDatabricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks'"
		},
		"ls_adls_sps_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagesiva12.dfs.core.windows.net/"
		},
		"On_Existence_of_the_file_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/090bb5ed-6ed2-4604-9640-1f6262471b2b/resourceGroups/REG-Siva/providers/Microsoft.Storage/storageAccounts/storagesiva12"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-rearn-2s_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_sps_accountKey": {
						"value": "[parameters('ls_adls_sps_accountKey')]"
					},
					"ls_sql_db_retaildb_connectionString": {
						"value": "[parameters('ls_sql_db_retaildb_connectionString')]"
					},
					"AzureDatabricks_accessToken": {
						"value": "[parameters('AzureDatabricks_accessToken')]"
					},
					"ls_adls_sps_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_sps_properties_typeProperties_url')]"
					},
					"On_Existence_of_the_file_properties_typeProperties_scope": {
						"value": "[parameters('On_Existence_of_the_file_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}